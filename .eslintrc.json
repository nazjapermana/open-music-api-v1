{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": ["off"],
        "quotes": ["error", "double"],
        "no-underscore-dangle": ["off"],
        "object-curly-newline": ["off"],
        "camelcase": ["off"],
        "comma-dangle": ["off"],
        "no-else-return": ["off"],
        "newline-per-chained-call": ["off"],
        "implicit-arrow-linebreak": ["off"],
        "operator-linebreak": ["off"]
    }
}
